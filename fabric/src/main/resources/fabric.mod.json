{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "${mod_author}"
    ],
    "contributors": [
        "ListerK",
        "HiWord9",
        "Eltarchik"
    ],
    "contact": {
        "sources": "${mod_sources}",
        "issues": "${mod_issue_tracker}"
    },
    "license": "${mod_license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "team.durt.enchantmentinfo.EnchantmentInfo"
        ],
        "client": [
            "team.durt.enchantmentinfo.EnchantmentInfoClient"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric-api": "*",
        "minecraft": "${fabric_minecraft_version_range}",
        "java": ">=${java_version}"
    },
    "suggests": {}
}
  